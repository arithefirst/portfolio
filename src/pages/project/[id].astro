---
export const prerender = true;
import Head from '../../layouts/Head.astro';
import Navbar from '../../layouts/Navbar.svelte';
import ProjectPage from '../../layouts/ProjectPage.svelte';

export function getStaticPaths() {
  return [{ params: { id: 'clifford' } }, { params: { id: 'rover' } }, { params: { id: 'spot' } }];
}

const { id } = Astro.params;
---

<html lang="en" data-navbar-override>
  <Head title={`arithefirst | ${id}`} description="tbd" />
  <body>
    <Navbar client:only />
    <ProjectPage client:load />
  </body>
</html>
